(ascmd:add '("~/Development/${1:SRMS_API}/.*\.${2:js}" "find $DIR$FILE | perl -pe 's/(\\/Users\\/${3:shun}(.*?)$)/\\1 shunsuke\\.takamiya\\@${4:jump}:\\/home\\/${5:shunsuke\\.takamiya}\\2/' | xargs rsync"))